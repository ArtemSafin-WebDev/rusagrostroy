{{#extend "layout"}}
{{#content "main"}}



<section class="catalog">

    <div class="container">
        {{> header }}
        <div class="catalog__breadcrumbs">
            <a href="#">
                Главная
            </a>
            <a href="#" class="active">
                Выбрать квартиру
            </a>
        </div>
        <h1 class="catalog__main-heading">
            Выбрать квартиру
        </h1>
        <div class="catalog__filters">


            <form action="/" class="filters__form extended-search-open" method="GET">
                <div class="filters__row">
                    <div class="filters__fields">
                        <div class="filters__field">
                            <div class="select">
                                <div class="select__label">
                                    Проект
                                </div>
                                <select name="project" class="js-custom-select">
                                    <option value="ЖК «Семейный»">ЖК «Семейный»</option>
                                    <option value="Другой ЖК»">Другой ЖК</option>
                                </select>
                            </div>

                        </div>
                        <div class="filters__field">
                            <div class="radios">
                                <div class="radios__label">
                                    Комнатность
                                </div>
                                <div class="radios__items">

                                    <label class="radios__item">
                                        <input type="radio" name="rooms" value="1" class="radios__item-input"
                                            checked="">
                                        <span class="radios__item-text">
                                            1
                                        </span>
                                    </label>
                                    <label class="radios__item">
                                        <input type="radio" name="rooms" value="2" class="radios__item-input">
                                        <span class="radios__item-text">
                                            2
                                        </span>
                                    </label>
                                    <label class="radios__item">
                                        <input type="radio" name="rooms" value="3" class="radios__item-input">
                                        <span class="radios__item-text">
                                            3
                                        </span>
                                    </label>
                                </div>
                            </div>
                        </div>
                        <div class="filters__field">
                            <div class="range-slider js-range-slider-double" data-min="1200000" data-max="6000000"
                                data-step="100000">
                                <div class="range-slider__label">
                                    Стоимость, руб
                                </div>
                                <div class="range-slider__inputs">
                                    <div class="range-slider__input-wrapper">
                                        <input type="tel" class="range-slider__input js-numeric-input-decimals">
                                    </div>
                                    <div class="range-slider__inputs-divider">

                                    </div>
                                    <div class="range-slider__input-wrapper">
                                        <input type="tel" class="range-slider__input js-numeric-input-decimals">
                                    </div>
                                </div>

                                <div class="range-slider__element">

                                </div>
                            </div>
                        </div>
                        <div class="filters__field">
                            <div class="range-slider js-range-slider-double" data-min="20" data-max="200" data-step="1">
                                <div class="range-slider__label">
                                    Площадь, м
                                </div>
                                <div class="range-slider__inputs">
                                    <div class="range-slider__input-wrapper range-slider__input-wrapper--small">
                                        <input type="tel" class="range-slider__input js-numeric-input">
                                    </div>
                                    <div class="range-slider__inputs-divider">

                                    </div>
                                    <div class="range-slider__input-wrapper range-slider__input-wrapper--small">
                                        <input type="tel" class="range-slider__input js-numeric-input">
                                    </div>
                                </div>

                                <div class="range-slider__element">

                                </div>
                            </div>
                        </div>
                    </div>
                    <button class="btn filters__submit">
                        Показать 355 квартир
                    </button>
                </div>
                <div class="filters__row filters__row--extended">
                    <div class="filters__fields filters__fields--short">
                        <div class="filters__field">
                            <div class="range-slider js-range-slider-double" data-min="1" data-max="20" data-step="1">
                                <div class="range-slider__label">
                                    Этаж
                                </div>
                                <div class="range-slider__inputs">
                                    <div class="range-slider__input-wrapper range-slider__input-wrapper--small">
                                        <input type="tel" class="range-slider__input js-numeric-input">
                                    </div>
                                    <div class="range-slider__inputs-divider">

                                    </div>
                                    <div class="range-slider__input-wrapper range-slider__input-wrapper--small">
                                        <input type="tel" class="range-slider__input js-numeric-input">
                                    </div>
                                </div>

                                <div class="range-slider__element">

                                </div>
                            </div>
                        </div>
                        <div class="filters__field">
                            <div class="select">
                                <div class="select__label">
                                    Срок сдачи
                                </div>
                                <select name="project" class="js-custom-select">
                                    <option value="1 кв 2023 г»">1 кв 2023 г</option>
                                    <option value="2 кв 2023 г">2 кв 2023 г</option>
                                </select>
                            </div>

                        </div>

                        <div class="filters__field">
                            <div class="select">
                                <div class="select__label">
                                    Позиция
                                </div>
                                <select name="position" class="js-custom-select">
                                    <option value="1»">Позиция 1 »</option>
                                    <option value="2»">Позиция 2</option>
                                </select>
                            </div>

                        </div>




                    </div>
                    <div class="filters__checkboxes">
                        <label class="filters__checkbox">
                            <input type="checkbox" class="filters__checkbox-input">
                            <span class="filters__checkbox-mark">

                            </span>
                            С отделкой
                        </label>
                        <label class="filters__checkbox">
                            <input type="checkbox" class="filters__checkbox-input">
                            <span class="filters__checkbox-mark">

                            </span>
                            Акция
                        </label>
                    </div>
                </div>
                <div class="filters__bottom-row">
                    <a href="#" class="filter__extended-search">
                        <span>
                            Расширенный поиск
                        </span>
                        <span>
                            Короткий поиск
                        </span>
                        <svg width="14" height="14" aria-hidden="true" class="icon-arrow-down">
                            <use xlink:href="#arrow-down"></use>
                        </svg>
                    </a>
                    <button type="reset" class="filters__reset-btn">
                        Сбросить поиск
                        <svg width="14" height="14" aria-hidden="true" class="icon-reset">
                            <use xlink:href="#reset"></use>
                        </svg>
                    </button>
                </div>
            </form>
        </div>
    </div>

    <div class="catalog__results catalog__results--projects">
        <div class="container">
            <div class="catalog__results-header catalog__results-header--projects">



